<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <padding
                android:bottom="1dp"
                android:right="1dp"
                android:left="1dp"
                android:top="1dp"/>
            <solid android:color="#99a69d9d"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#AEA5A5"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>